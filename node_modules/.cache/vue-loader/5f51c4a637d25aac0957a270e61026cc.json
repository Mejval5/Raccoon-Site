ourceRoot":"src/layouts/home","sourcesContent":["<template>\n  <div>\n    <v-app-bar\n      id=\"home-app-bar\"\n      app\n      color=\"white\"\n      elevation=\"1\"\n      height=\"80\"\n    >\n      <base-img\n        :src=\"require('@/assets/racc.svg')\"\n        class=\"mr-3\"\n        contain\n        max-width=\"52\"\n        width=\"100%\"\n      />\n\n      <v-tab\n            active-class=\"text--primary\"\n            class=\"font-weight-bold headline\"\n            min-width=\"96\"\n            text\n          >\n          Raccoon site\n          </v-tab>\n<!--\n      <base-img\n        :src=\"require('@/assets/zero-logo-light.svg')\"\n        contain\n        max-width=\"128\"\n        width=\"100%\"\n      />\n-->\n      <v-spacer />\n\n      <div>\n        <v-tabs\n          class=\"hidden-sm-and-down\"\n          optional\n        >\n          <v-tab\n            v-for=\"(name, i) in items\"\n            :key=\"i\"\n            :to=\"{ name }\"\n            :exact=\"name === 'Home'\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text\n          >\n            {{ name }}\n          </v-tab>\n        </v-tabs>\n      </div>\n\n      <v-app-bar-nav-icon\n        class=\"hidden-md-and-up\"\n        @click=\"drawer = !drawer\"\n      />\n    </v-app-bar>\n\n    <home-drawer\n      v-model=\"drawer\"\n      :items=\"items\"\n    />\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'HomeAppBar',\n\n    components: {\n      HomeDrawer: () => import('./Drawer'),\n    },\n\n    data: () => ({\n      drawer: null,\n      items: [\n        'Home',\n        'About',\n        'Games',\n        'Blog',\n        'Contact',\n      ],\n    }),\n  }\n</script>\n\n<style lang=\"sass\">\n  #home-app-bar\n    .v-tabs-slider\n      max-width: 24px\n      margin: 0 auto\n\n    .v-tab\n      &::before\n        display: none\n</style>\n"]}]}